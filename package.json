{
  "name": "api-v2",
  "version": "13.0.0",
  "engines": {
    "node": ">=16.*"
  },
  "type": "module",
  "scripts": {
    "clean": "del-cli dist",
    "build": "npm run clean && tsc",
    "build:production": "npm run prestart && npm prune --production",
    "watch": "tsc --w",
    "prestart": "npm run build && func extensions install",
    "start:host": "func start --cors *",
    "start": "npm-run-all --parallel start:host watch",
    "postinstall": "node patch.js",
    "open-api": "node .openAPI/swagger-node-middleware.js",
    "open-api:generate": "npx .openAPI/cli --config .openAPI/cli/config.json"
  },
  "description": "",
  "devDependencies": {
    "@azure/functions": "^3.2.0",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "azurite": "^3.20.1",
    "del-cli": "^5.0.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@azure/identity": "^3.0.1",
    "@azure/storage-blob": "^12.12.0",
    "@azure/storage-queue": "^12.11.0",
    "ajv": "^8.11.0",
    "chrome-launcher": "^0.15.1",
    "durable-functions": "^2.1.0",
    "form-data": "^4.0.0",
    "formdata-node": "^5.0.0",
    "fs-extra": "^10.1.0",
    "hint": "^7.1.3",
    "http-proxy": "^1.18.1",
    "jimp": "^0.16.2",
    "jsdom": "^21.1.0",
    "jszip": "^3.10.1",
    "klaw": "^4.0.1",
    "lighthouse": "10.0.1",
    "node-fetch": "^3.3.0",
    "puppeteer": "19.7.2",
    "pwabuilder-lib": "^2.1.12",
    "svgjs": "^2.6.2",
    "swagger-jsdoc": "^7.0.0-rc.6",
    "swagger-ui-dist": "^4.15.5",
    "utf8": "^3.0.0",
    "yaml": "^2.2.1"
  }
}
