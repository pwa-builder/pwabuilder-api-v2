{
  "name": "api-v2",
  "version": "2.1.0",
  "engines": {
    "node": ">=18.*"
  },
  "type": "module",
  "scripts": {
    "clean": "del-cli dist && del-cli temp",
    "build": "npm run clean && tsc",
    "build:production": "npm run prestart && npm prune --production",
    "watch": "tsc --w",
    "prestart": "npm run build && func extensions install",
    "start:host": "func start --cors *",
    "start": "npm-run-all --parallel start:host watch",
    "postinstall": "node patch.js",
    "open-api": "node .openAPI/swagger-node-middleware.js",
    "open-api:generate": "npx .openAPI/cli --config .openAPI/cli/config.json",
    "test": "npx playwright test"
  },
  "description": "",
  "devDependencies": {
    "@azure/functions": "^3.5.1",
    "@playwright/test": "^1.31.2",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "del-cli": "^5.0.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.12.0",
    "ajv": "^8.11.0",
    "azure-functions-core-tools": "^4.0.5148",
    "chrome-launcher": "^0.15.1",
    "form-data": "^4.0.0",
    "hint": "^7.1.3",
    "jimp": "^0.22.5",
    "jsdom": "^21.1.0",
    "jszip": "^3.10.1",
    "lighthouse": "10.0.2",
    "node-fetch": "^3.3.1",
    "puppeteer": "19.7.2",
    "pwabuilder-lib": "^2.1.12",
    "strip-json-comments": "^5.0.0",
    "swagger-jsdoc": "^7.0.0-rc.6",
    "swagger-ui-dist": "^4.15.5",
    "yaml": "^2.2.1"
  }
}
