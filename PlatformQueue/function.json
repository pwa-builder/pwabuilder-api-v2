{
  "bindings": [
    {
      "queueName": "platform-queue",
      "connection": "AzureWebJobsStorage",
      "name": "queueItem",
      "type": "queueTrigger",
      "direction": "in"
    },
    {
      "name": "storeRead",
      "type": "blob",
      "path": "{queueTrigger}",
      "connection": "AzureWebJobsStorage",
      "direction": "in"
    },
    {
      "name": "storeWrite",
      "type": "blob",
      "path": "{queueTrigger}",
      "connection": "AzureWebJobsStorage",
      "direction": "out"
    }
  ],
  "scriptFile": "../dist/PlatformQueue/index.js"
}
