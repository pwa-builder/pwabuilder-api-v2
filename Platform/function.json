{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": ["get", "post"]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "queueName": "platform-queue",
      "connection": "pwabuilderstore",
      "name": "queueItem",
      "type": "queue",
      "direction": "out"
    }
  ],
  "scriptFile": "../dist/Platform/index.js"
}
